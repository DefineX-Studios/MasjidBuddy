{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "typeRoots": ["./src/types", "./types"],
    "paths": {
      "@/*": ["./src/*"],
      "@env": ["./src/core/env.js"],
      "*": ["node_modules/*"],
      "@react-native-fbsdk-next": ["types/react-native-fbsdk-next.d.ts"]
    },
    "esModuleInterop": true,
    "checkJs": true
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "docs",
    "cli",
    "android",
    "ios",
    "lint-staged.config.js"
  ]
}
